{
  "manifest_version": 2,
  "name": "Check mate",
  "version": "1.0",
  "description": "A basic Firefox extension.",
  "icons": {
    "48": "icons/logo.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/logo.png"
    },
    "default_title": "Click me!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
